pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
	parameters{
		string(name: 'TOEXECUTE', defaultName: 'defaultExecute', description: 'ToExecute?')
	}
    stages {
        stage('build') {
	when {
		environment name: 'TOEXECUTE', value: 'build'
		}
	}
	stage('test') {
                when {
			environmet name:'TOEXECUTE', value: 'test'
		}
            }
        }
   }
